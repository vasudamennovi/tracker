<core:FragmentDefinition xmlns="sap.m" 	xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:tnt="sap.tnt">
	
	
	
	<Popover
		id="myPopover"
		title="Tickect Aging"
		class="sapUiContentPadding"
		showHeader="true"
		placement="Right"
		verticalScrolling="true"
		initialFocus="action">
		<VBox>
		<List
		
		items="{
			path: 'dashboardAgingLog>/'
		}" >
		<StandardListItem
			title="{dashboardAgingLog>Name}"
			description="{dashboardAgingLog>Aging}">
		
			</StandardListItem>
	</List>
		</VBox>
			
	</Popover>
</core:FragmentDefinition>