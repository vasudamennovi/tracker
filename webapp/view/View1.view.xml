<mvc:View controllerName="ztpr.zticketprocessing.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:tnt="sap.tnt"
    xmlns:f="sap.f"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:u="sap.ui.unified">

    <f:DynamicPage id="dynamicPageId" headerExpanded="true">
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <Title text="Development Tracker - Manage"/>
                </f:heading>
            </f:DynamicPageTitle>
        </f:title>
        <f:header>
            <f:DynamicPageHeader >
                <f:content>
                    <fb:FilterBar id="filterBar" useToolbar="false" search="onPressGo" showClearOnFB="true" clear="onPressClear">
                        <fb:filterGroupItems>
                            <fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="A1" label="RICEF ID">
                                <fb:control>
                                    <MultiComboBox
                        id = "TickNum" selectionFinish="onSelectTicNum"
                        width = "260px" items="{
                            path: 'datamodel1>/'
                            }">
                                        <core:Item key="{datamodel1>RICEF_ID}" text="{datamodel1>RICEF_ID}" />
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="A2" label="Description">
                                <fb:control>
                                    <MultiComboBox
                        id = "Descrip" selectionFinish="onSelectDescrip"
                        width = "260px" items="{
                            path: 'desModel>/'
                            }">
                                        <core:Item key="{desModel>}" text="{desModel>}" />
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="A9" label="Region">
                                <fb:control>
                                    <MultiComboBox
                        id = "regionId" selectionFinish="onSelectBU"
                        width = "260px" items="{
                            path: 'regionModel>/'
                            }">
                                        <core:Item key="{regionModel>InputTc}" text="{regionModel>InputTc}" />
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="A3" label="Functional Consultant">
                                <fb:control>
                                    <MultiComboBox
                        id = "FuncCons" selectionFinish="onSelectFuncCons"
                        width = "260px" items="{
                            path: 'funcConstModel>/'
                            }">
                                        <core:Item key="{funcConstModel>Name}" text="{funcConstModel>Name}" />
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="A4" label="Technical Consultant">
                                <fb:control>
                                    <MultiComboBox
                        id = "TechCons" selectionFinish="onSelecttechCons"
                        width = "260px" items="{
                            path: 'techConstModel>/'
                            }">
                                        <core:Item key="{techConstModel>Name}" text="{techConstModel>Name}" />
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="A5" label="Priority">
                                <fb:control>
                                    <MultiComboBox
                        id = "Prio" selectionFinish="onSelectPrio"
                        width = "260px" items="{
                            path: 'prioModel>/'
                            }">
                                        <core:Item key="{prioModel>InputTc}" text="{prioModel>InputTc}" />
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="A6" label="Complexity">
                                <fb:control>
                                    <MultiComboBox
                        id = "Complex" selectionFinish="onSelectComplex"
                        width = "260px" items="{
                            path: 'compModel>/'
                            }">
                                        <core:Item key="{compModel>InputTc}" text="{compModel>InputTc}" />
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="A8" label="Ticket Status" visible="false">
                                <fb:control>
                                    <MultiComboBox
                        id = "TicSts" selectionFinish="onSelectTicSts"
                        width = "260px" items="{
                            path: 'tickStsModel>/'
                            }">
                                        <core:Item key="{tickStsModel>}" text="{tickStsModel>}" />
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="A7" label="Development Status">
                                <fb:control>
                                    <MultiComboBox
                        id = "DevSts" selectionFinish="onSelectDevSts"
                        width = "260px" items="{
                            path: 'devStsModel>/'
                            }">
                                        <core:Item key="{devStsModel>InputTc}" text="{devStsModel>InputTc}" />
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                        </fb:filterGroupItems>
                    </fb:FilterBar>
                </f:content>
            </f:DynamicPageHeader>
        </f:header>

        <f:content>
            <Table id="dashboard" items="{datamodel1>/}" growing="true" sticky="ColumnHeaders" alternateRowColors="true">
                <headerToolbar>
                    <OverflowToolbar >
                        <Label design="Bold" text="Items ({oItemLenModel>/})"></Label>
                        <ToolbarSpacer/>
                        <SearchField id="search" liveChange="onSearch" width="30%" />
                        <Button press="onExcelExport" icon="sap-icon://excel-attachment" />

                    </OverflowToolbar>
                </headerToolbar>
                <columns>

                    <Column width="10%">
                        <Label text="RICEF ID" />
                    </Column>
                    <Column hAlign="Center">
                        <Label text="Description" />
                    </Column>
                    <Column hAlign="Center">
                        <Label text="Region" />
                    </Column>
                    <Column hAlign="Center">
                        <Label text="Functional Consultant" />
                    </Column>
                    <Column hAlign="Center">
                        <Label text="Technical Consultant"/>
                    </Column>
                    <Column hAlign="Center">
                        <Label text="FS Target Com.Date"/>
                    </Column>
                    <Column hAlign="Center" visible="false">
                        <Label text="Dev Target Com.Date"/>
                    </Column>

                    <Column hAlign="Center">
                        <Label text="Priority"/>
                    </Column>
                    <Column hAlign="Center">
                        <Label text="Complexity" />
                    </Column>
                    <Column hAlign="Center">
                        <Label text="Aging"/>
                    </Column>
                    <Column hAlign="Center" visible="false">
                        <Label text="Ticket Status" />
                    </Column>
                    <Column hAlign="Center">
                        <Label text="Defects" />
                    </Column>

                    <Column hAlign="Center">
                        <Label text="DevStatus"/>
                    </Column>

                    <Column hAlign="Center">
                        <Label text="Ref.Copy" />
                    </Column>


                </columns>
                <items>
                    <ColumnListItem type="Navigation" press="onRowSelect">

                        <Link text="{datamodel1>RICEF_ID}" press="NavtoView2"   />

                        <Text text="{datamodel1>Description}"/>
                        <Text text="{datamodel1>BusnUnit}"/>

                        <HBox class="tableDate">
                            <core:Icon src="sap-icon://employee" color="blue" class="funcSpace"/>
                            <Text text="{datamodel1>FuncConst}"/>
                        </HBox>

                        <HBox class="tableDate">
                            <core:Icon src="sap-icon://employee" color="blue" class="techSpace"/>
                            <Text text="{datamodel1>TechPs}"/>
                        </HBox>

                        <!-- <Text text="{datamodel1>FuncConst}"/> -->
                        <HBox class="tableDate">
                            <core:Icon src="sap-icon://appointment-2" color="green" class="dateSpace" />
                            <Text text="{datamodel1>FsReceDate}"/>
                        </HBox>

                        <!--  <Text text="{datamodel1>TechPs}"/> -->
                        <!--  <Text text="{datamodel1>FsTargetCom}"/> -->


                        <HBox class="tableDate">
                            <core:Icon src="sap-icon://appointment-2" color="green"/>
                            <Text text="{datamodel1>DevTargetDate}"/>
                        </HBox>

                        <tnt:InfoLabel text="{datamodel1>Priority}" displayOnly="false" colorScheme="{ parts: [ {path: 'datamodel1>Priority'} ], formatter: '.Priority' }" />

                        <tnt:InfoLabel text="{datamodel1>Complexity}" displayOnly="false" colorScheme="{ parts: [ {path: 'datamodel1>Complexity'} ], formatter: '.Complexity' }"/>

                        <!--  <Text text="{datamodel1>Aging}"/> -->
                        <Link text="{datamodel1>Aging}" press="onQuikView" ariaHasPopup="Dialog"/>
                        <!--  <tnt:InfoLabel text="{datamodel1>Aging}" displayOnly="false" colorScheme="{ parts: [ {path: 'datamodel1>Aging'} ], formatter: '.statusColor' }"/> -->

                        <tnt:InfoLabel text="{datamodel1>TicketStatus}" displayOnly="false" colorScheme="{ parts: [ {path: 'datamodel1>TicketStatus'} ], formatter: '.statusColor' }"/>
                        <Text text="{datamodel1>DefectCount}"></Text>

                        <Text text="{datamodel1>DevStatus}"/>



                        <Button icon="sap-icon://copy" type="Emphasized" press="Ref_Ok" />


                    </ColumnListItem>
                </items>

            </Table>


        </f:content>
    </f:DynamicPage>


</mvc:View>



























